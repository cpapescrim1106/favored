# Favored Environment Variables

# Database
POSTGRES_USER=favored
POSTGRES_PASSWORD=change_me_in_production
POSTGRES_DB=favored
DATABASE_URL=postgresql://favored:change_me_in_production@localhost:5432/favored

# Ports (development)
WEB_PORT=3000
DB_PORT=5432
PRISMA_STUDIO_PORT=5555

# Worker settings
SCAN_INTERVAL="*/10 * * * *"

# Polymarket CLOB API (L2 credentials)
# Required for MVP1+ (real trading)
# POLYMARKET_API_KEY=
# POLYMARKET_API_SECRET=
# POLYMARKET_PASSPHRASE=

# Wallet (for signing orders)
# Required for MVP1+ (real trading)
# WALLET_PRIVATE_KEY=

# Optional
LOG_LEVEL=info
